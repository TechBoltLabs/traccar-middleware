# Required settings
T2M_AUTH_SECRET=your_secret_auth_key_here
MQTT_BROKER=your.mqtt.broker.com
MQTT_USERNAME=your_mqtt_username
MQTT_PASSWORD=your_mqtt_password

# Optional settings with defaults
T2M_PORT=7777
T2M_LOG_FILENAME=logs/t2m.log
T2M_LOG_LEVEL=INFO
MQTT_PORT=1883
MQTT_TOPIC=t2m/homeassistant/location/
MQTT_CLIENT_ID=traccar2mqtt-12345

# HTTP endpoint configuration (optional)
# Base URL without the object_id - it will be appended automatically
# Example: if your API is at https://api.example.com/api/v1/location/{object_id}
# then set HTTP_ENDPOINT=https://api.example.com/api/v1/location
HTTP_ENDPOINT=https://api.example.com/api/v1/location
HTTP_API_KEY=your_api_key_here
# Additional HTTP headers as JSON (optional)
HTTP_HEADERS={"X-Custom-Header": "value", "User-Agent": "Traccar2MQTT/1.0"}